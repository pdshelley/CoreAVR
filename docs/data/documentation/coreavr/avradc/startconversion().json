{"identifier":{"url":"doc:\/\/CoreAVR\/documentation\/CoreAVR\/AVRADC\/startConversion()","interfaceLanguage":"swift"},"sections":[],"primaryContentSections":[{"kind":"declarations","declarations":[{"platforms":["macOS"],"languages":["swift"],"tokens":[{"kind":"keyword","text":"static"},{"text":" ","kind":"text"},{"kind":"keyword","text":"func"},{"text":" ","kind":"text"},{"text":"startConversion","kind":"identifier"},{"kind":"text","text":"()"}]}]},{"content":[{"type":"heading","anchor":"discussion","level":2,"text":"Discussion"},{"inlineContent":[{"type":"text","text":"This function ses the ADC Start Conversion (ADSC) bit to 1. Becasue writing 0 to this bit has no effect this code pattern will disallow that behaviour."}],"type":"paragraph"},{"inlineContent":[{"text":"In Single Conversion mode, write this bit to one to start each conversion. In Free Running mode,","type":"text"},{"type":"text","text":" "},{"type":"text","text":"write this bit to one to start the first conversion. The first conversion after ADSC has been written"},{"type":"text","text":" "},{"text":"after the ADC has been enabled, or if ADSC is written at the same time as the ADC is enabled, will take","type":"text"},{"type":"text","text":" "},{"type":"text","text":"25 ADC clock cycles instead of the normal 13. This first conversion performs initialization of the ADC."}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"ADSC will read as one as long as a conversion is in progress. When the conversion is complete,"},{"type":"text","text":" "},{"text":"it returns to zero. Writing zero to this bit has no effect.","type":"text"}],"type":"paragraph"}],"kind":"content"}],"abstract":[{"text":"ADC Start Conversion","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"See ATMega328p Datasheet Section 24.9.2."},{"text":" ","type":"text"},{"type":"text","text":"ADSC is bit 6 on ADCSRA."}],"hierarchy":{"paths":[["doc:\/\/CoreAVR\/documentation\/CoreAVR","doc:\/\/CoreAVR\/documentation\/CoreAVR\/AVRADC"]]},"variants":[{"paths":["\/documentation\/coreavr\/avradc\/startconversion()"],"traits":[{"interfaceLanguage":"swift"}]}],"kind":"symbol","metadata":{"externalID":"s:7CoreAVR6AVRADCPAAE15startConversionyyFZ","extendedModule":"CoreAVR","modules":[{"name":"CoreAVR"}],"fragments":[{"text":"static","kind":"keyword"},{"text":" ","kind":"text"},{"kind":"keyword","text":"func"},{"kind":"text","text":" "},{"kind":"identifier","text":"startConversion"},{"kind":"text","text":"()"}],"role":"symbol","title":"startConversion()","symbolKind":"method","roleHeading":"Type Method"},"schemaVersion":{"patch":0,"major":0,"minor":3},"references":{"doc://CoreAVR/documentation/CoreAVR":{"abstract":[],"type":"topic","role":"collection","title":"CoreAVR","identifier":"doc:\/\/CoreAVR\/documentation\/CoreAVR","kind":"symbol","url":"\/documentation\/coreavr"},"doc://CoreAVR/documentation/CoreAVR/AVRADC":{"url":"\/documentation\/coreavr\/avradc","identifier":"doc:\/\/CoreAVR\/documentation\/CoreAVR\/AVRADC","kind":"symbol","role":"symbol","navigatorTitle":[{"kind":"identifier","text":"AVRADC"}],"type":"topic","abstract":[],"title":"AVRADC","fragments":[{"kind":"keyword","text":"protocol"},{"text":" ","kind":"text"},{"kind":"identifier","text":"AVRADC"}]},"doc://CoreAVR/documentation/CoreAVR/AVRADC/startConversion()":{"title":"startConversion()","type":"topic","abstract":[{"type":"text","text":"ADC Start Conversion"},{"text":" ","type":"text"},{"type":"text","text":"See ATMega328p Datasheet Section 24.9.2."},{"type":"text","text":" "},{"type":"text","text":"ADSC is bit 6 on ADCSRA."}],"url":"\/documentation\/coreavr\/avradc\/startconversion()","identifier":"doc:\/\/CoreAVR\/documentation\/CoreAVR\/AVRADC\/startConversion()","role":"symbol","fragments":[{"text":"static","kind":"keyword"},{"kind":"text","text":" "},{"text":"func","kind":"keyword"},{"text":" ","kind":"text"},{"kind":"identifier","text":"startConversion"},{"text":"()","kind":"text"}],"kind":"symbol"}}}